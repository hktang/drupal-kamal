# Copy this file to `.env` and fill in values.
# Then load into your shell before running Kamal commands:
#
#   set -a; source .env; set +a
#
# These are read by .kamal/secrets when using the "ENV variables" injection method.

# Local Registry settings
LOCAL_REGISTRY_USRNAME=xxxxxxxxxxx
LOCAL_REGISTRY_PASSWORD=xxxxxxxxxxxxxxxxxxxxxx

# Drupal settings
DRUPAL_HASH_SALT=xxxxxxxxxxxxxxxxxxxx

# Credentials for the MySQL accessory
MYSQL_ROOT_PASSWORD=xxxxxxxxxxxxxxxxxxxxxx
MYSQL_PASSWORD=xxxxxxxxxxxxxxxxxxxxxxxx

# If you use a secret manager, specify the login email and project name here.
SECRET_MANAGER_EMAIL=xxxxxxxxxxxxxxxxxxxxxxxxxx
SECRET_MANAGER_PROJECT_NAME=xxxxxxxxx
