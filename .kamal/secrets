# Extract from Password Manager
SECRETS=$(kamal secrets fetch --adapter gcp --account "$SECRET_MANAGER_EMAIL" --from "$SECRET_MANAGER_PROJECT_NAME" SINGLE_USE___DEMO_MYSQL_HOST SINGLE_USE___DEMO_MYSQL_PASSWORD)

# Assign to environment variables
MYSQL_HOST=$(kamal secrets extract SINGLE_USE___DEMO_MYSQL_HOST $SECRETS)
MYSQL_PASSWORD=$(kamal secrets extract SINGLE_USE___DEMO_MYSQL_PASSWORD $SECRETS)
